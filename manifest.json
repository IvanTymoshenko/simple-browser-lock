{
  "manifest_version": 3,
  "name": "Simple Browser Lock",
  "version": "1.0.5",
  "description": "Lock your browser with a password. Secure and simple.",
  "author": "Ivan Tymoshenko",
  "permissions": [
    "storage",
    "tabs",
    "windows",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}